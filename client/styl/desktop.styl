/* dialog */

.shadow {
	display: none;
	position: absolute;
	top: 0;
	width: 100%;
	height: 100%;
	background: rgba(000, 020, 023, 0.7);
	z-index: 1000;
}
.inner-shadow {
	width: 50%;
	height: 50%;
	overflow: auto;
	color: white;
	margin: auto;
	position: absolute;
	top: 0; 
	left: 0; 
	bottom: 0; 
	right: 0;
}
.dialog-content {
	position: relative;
	width: 400px;
	margin-left: auto;
	margin-right: auto;
	height: 149px;
	background-image: url('../img/dialog.png');
	background-color: rgba(000, 000, 000, 0.1);
	border-radius: 8px;
	margin-top: 12%;
}
.dialog-text {
	cursor: default;
	color: black;
	font: bold 12px Verdana;
	width: 360px;
	margin-right: auto;
	margin-left: auto;
	position: relative;
	top: 25px;
}
.dialog-content .buttons {
	height: 30px;
	position: absolute;
	bottom: 15px;
	width: 97%;
}
.dialog-content .buttons .btn-warning {
	color: black;
}
.dialog-content .buttons button {
	margin-right: 2px;
	font: bold 12px Verdana;
}
.dialog-close {
	margin-top: 3px;
	float: right;
	margin-right: 3px;
	background: url('../img/dialog-close.png');
	width: 15px;
	height: 14px;
	cursor: pointer;
}
.dialog-close:hover {
	background-position: 15px;
}
.dialog-close:active {
	background-position: 30px;
}
.dialog-icon {
	width: 16px;
	height: 16px;
	margin-left: 6px;
	margin-top: -5px;
}
.modal-dialog {
	margin-top: 20% !important;
	width: 280px;
	color: #970101 !important;
	font: 12px verdana;
	text-align: center;
}
.modal-content {
	background: rgba(999, 999, 999, 0.7) !important;
}

